{
  "buildCommand": null,
  "outputDirectory": ".",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://cold-case-crawler-api.up.railway.app/api/:path*"
    },
    {
      "source": "/membership/:path*", 
      "destination": "https://cold-case-crawler-api.up.railway.app/membership/:path*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "X-Frame-Options", "value": "DENY" }
      ]
    }
  ]
}
